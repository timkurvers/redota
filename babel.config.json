{
  "plugins": [
    "@babel/plugin-syntax-import-assertions"
  ],
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
        "browsers": [
          "last 2 chrome versions",
          "last 2 edge versions",
          "last 2 firefox versions",
          "last 2 safari versions"
        ]
      },
      "modules": "auto"
    }],
    "@babel/preset-react"
  ]
}
